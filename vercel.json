{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["hnd1"],
  "functions": {
    "src/app/api/documents/upload/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "src/app/api/documents/*/parse/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "src/app/api/documents/*/process/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}

